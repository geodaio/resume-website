{
  "functions": {
    "**/**/*": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "(.*)/aboutme.php(.*)", "dest": "api/php/aboutme.php" },
    { "src": "(.*)/contact.php(.*)", "dest": "api/php/contact.php" },
    { "src": "(.*)/resume.php(.*)", "dest": "api/php/resume.php" },
    { "src": "(.*)/work.php(.*)", "dest": "api/php/work.php" },
    { "src": "(.*)/sendmessage.php(.*)", "dest": "api/php/sendmessage.php" },
    { "src": "/(.*)", "dest": "api/php/index.php" }
  ]
}
