{
  "functions": {
    "**/**/*": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "(.*)/aboutme.php(.*)", "dest": "(.*)/aboutme.php" },
    { "src": "(.*)/contact.php(.*)", "dest": "(.*)/contact.php" },
    { "src": "(.*)/resume.php(.*)", "dest": "(.*)/resume.php" },
    { "src": "(.*)/work.php(.*)", "dest": "(.*)/work.php" },
    { "src": "(.*)/sendmessage.php(.*)", "dest": "(.*)/sendmessage.php" },
    { "src": "/(.*)", "dest": "(.*)/index.php" }
  ]
}
